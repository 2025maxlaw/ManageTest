<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.bussiness.mapper.TUserIpWhiteMapper">
    
    <resultMap type="TUserIpWhite" id="TUserIpWhiteResult">
        <result property="id"    column="id"    />
        <result property="uid"    column="uid"    />
        <result property="ip"    column="ip"    />
        <result property="createTime"    column="create_time"    />
        <result property="createBy"    column="create_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectTUserIpWhiteVo">
        select id, uid, ip, create_time, create_by, update_time, update_by, remark from t_user_ip_white
    </sql>

    <select id="selectTUserIpWhiteList" parameterType="TUserIpWhite" resultMap="TUserIpWhiteResult">
        <include refid="selectTUserIpWhiteVo"/>
        <where>  
            <if test="uid != null "> and uid = #{uid}</if>
            <if test="ip != null  and ip != ''"> and ip = #{ip}</if>
        </where>
    </select>
    
    <select id="selectTUserIpWhiteById" parameterType="Long" resultMap="TUserIpWhiteResult">
        <include refid="selectTUserIpWhiteVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTUserIpWhite" parameterType="TUserIpWhite" useGeneratedKeys="true" keyProperty="id">
        insert into t_user_ip_white
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="uid != null">uid,</if>
            <if test="ip != null and ip != ''">ip,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="uid != null">#{uid},</if>
            <if test="ip != null and ip != ''">#{ip},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateTUserIpWhite" parameterType="TUserIpWhite">
        update t_user_ip_white
        <trim prefix="SET" suffixOverrides=",">
            <if test="uid != null">uid = #{uid},</if>
            <if test="ip != null and ip != ''">ip = #{ip},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTUserIpWhiteById" parameterType="Long">
        delete from t_user_ip_white where id = #{id}
    </delete>

    <delete id="deleteTUserIpWhiteByIds" parameterType="String">
        delete from t_user_ip_white where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>