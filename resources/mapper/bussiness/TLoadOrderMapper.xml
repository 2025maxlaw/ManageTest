    <sql id="selectTLoadOrderVo">
        select id, pro_id, user_id, create_time, amount, rate, interest, status, final_repay_time, disburse_time, return_time, disburse_amount, admin_parent_ids, card_url, card_back_url, capital_url, license_url, order_no, cycle_type, remark, create_by, update_by, update_time, search_value from t_load_order
    </sql>

    <select id="selectTLoadOrderList" parameterType="TLoadOrder" resultMap="TLoadOrderResult">
        <include refid="selectTLoadOrderVo"/>
        <where>  
            <if test="proId != null "> and pro_id = #{proId}</if>
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="amount != null "> and amount = #{amount}</if>
            <if test="rate != null "> and rate = #{rate}</if>
            <if test="interest != null "> and interest = #{interest}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="finalRepayTime != null "> and final_repay_time = #{finalRepayTime}</if>
            <if test="disburseTime != null "> and disburse_time = #{disburseTime}</if>
            <if test="returnTime != null "> and return_time = #{returnTime}</if>
            <if test="disburseAmount != null "> and disburse_amount = #{disburseAmount}</if>
            <if test="adminParentIds != null  and adminParentIds != ''"> and admin_parent_ids = #{adminParentIds}</if>
            <if test="cardUrl != null  and cardUrl != ''"> and card_url = #{cardUrl}</if>
            <if test="cardBackUrl != null  and cardBackUrl != ''"> and card_back_url = #{cardBackUrl}</if>
